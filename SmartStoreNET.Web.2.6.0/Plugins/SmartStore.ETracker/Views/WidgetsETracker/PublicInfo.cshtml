@model SmartStore.ETracker.Models.PublicInfoModel

@{
    Layout = "";
}

@using SmartStore.Core;
@using SmartStore.Core.Infrastructure;
@using SmartStore.Web.Framework;
@using SmartStore.Web.Framework.Theming;

@{
    int etLevel = 0;

    string area = "Seite";
    if (this.CurrentCategoryId > 0)
    {
        area = "Warengruppe";
    }
    else if (this.CurrentProductId > 0)
    {
        area = "Produkt";
    }
    else if (this.CurrentManufacturerId > 0)
    {
        area = "Hersteller";
    }

    string pageName = "'Home'";
    if (!IsHomePage)
    {
        pageName = "document.title.substring(0,60)";
    }
}
