@using SmartStore.ShopConnector.Models;
@model ProductImportCompletedModel
@{
	Layout = null;
	Html.AddCssFileParts(true, Url.Content("~/Plugins/SmartStore.ShopConnector/Content/smartstore.shopconnector.css"));
}

<div>
	<p>@T("Plugins.SmartStore.ShopConnector.ImportCompleted")</p>

	<div>
		@if (Model.ImportLogFileUrl.HasValue())
		{
			<a href="@Model.ImportLogFileUrl" class="btn" target="LogFile">
				<i class="fa fa-fw fa-file-text-o"></i>&nbsp;@T("Plugins.SmartStore.ShopConnector.Action.ImportLogFile")
			</a>
		}
	</div>
</div>